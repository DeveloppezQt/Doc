<db:section xml:id="relativeX-prop">
<db:title>relativeX : real</db:title>
<db:fieldsynopsis>
<db:type>real</db:type>
<db:varname>relativeX</db:varname>
<db:modifier>writable</db:modifier>
<db:synopsisinfo role="access">public</db:synopsisinfo>
