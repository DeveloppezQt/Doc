<db:title>Property Documentation</db:title>
<db:section xml:id="certificates-prop">
<db:title>certificates : list&lt;WebEngineClientCertificateOption&gt;</db:title>
<db:fieldsynopsis>
<db:type>list&lt;WebEngineClientCertificateOption&gt;</db:type>
<db:varname>certificates</db:varname>
<db:modifier>writable</db:modifier>
