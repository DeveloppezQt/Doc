<db:section xml:id="details">
<db:title>Detailed Description</db:title>
<db:para>The example below creates a path consisting of a straight line from 0,100 to 200,100:</db:para>
<db:programlisting language="qml">Path {
    startX: 0; startY: 100
    PathLine { x: 200; y: 100 }
}
</db:programlisting>
