<db:section xml:id="item-var">
<db:title>QQuickItem * ItemChangeData::item</db:title>
<db:fieldsynopsis>
<db:type>QQuickItem *</db:type>
<db:varname>item</db:varname>
<db:synopsisinfo role="access">public</db:synopsisinfo>
<db:synopsisinfo role="status">active</db:synopsisinfo>
<db:synopsisinfo role="threadsafeness">unspecified</db:synopsisinfo>
</db:fieldsynopsis>
<db:para>The item that has been added or removed as a <db:link xlink:href="qquickitem.xml#childItems">child</db:link>, or the new <db:link xlink:href="qquickitem.xml#parent-prop">parent</db:link>.</db:para>
