<db:section xml:id="volumeOverride-prop">
<db:title>volumeOverride : Qt3DRender::QLevelOfDetailBoundingSphere</db:title>
<db:fieldsynopsis>
<db:modifier>(Qt property)</db:modifier>
<db:type>Qt3DRender::QLevelOfDetailBoundingSphere</db:type>
