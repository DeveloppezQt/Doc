<db:title>Member Type Documentation</db:title>
<db:section xml:id="Error-enum">
<db:title>enum QWebEngineCertificateError::Error</db:title>
<db:enumsynopsis>
<db:enumname>Error</db:enumname>
<db:enumitem>
<db:enumidentifier>SslPinnedKeyNotInCertificateChain</db:enumidentifier>
<db:enumvalue>-150</db:enumvalue>
</db:enumitem>
<db:enumitem>
<db:enumidentifier>CertificateCommonNameInvalid</db:enumidentifier>
<db:enumvalue>-200</db:enumvalue>
</db:enumitem>
<db:enumitem>
