<db:methodsynopsis>
<db:void/>
<db:methodname>filterIndices</db:methodname>
<db:methodparam>
<db:type>const QString &amp;</db:type>
<db:parameter>filter</db:parameter>
</db:methodparam>
<db:methodparam>
