<db:programlisting language="cpp">import QmlProject 1.1

Project {
    mainFile: &quot;qml-i18n.qml&quot;

    /* Include .qml, .js, and image files from current directory and subdirectories */
    QmlFiles {
        directory: &quot;.&quot;
    }
    JavaScriptFiles {
        directory: &quot;.&quot;
    }
    ImageFiles {
        directory: &quot;.&quot;
    }
}

