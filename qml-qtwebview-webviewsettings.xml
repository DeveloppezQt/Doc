<db:title>Property Documentation</db:title>
<db:section xml:id="allowFileAccess-prop">
<db:title>allowFileAccess : bool</db:title>
<db:fieldsynopsis>
<db:type>bool</db:type>
<db:varname>allowFileAccess</db:varname>
<db:modifier>writable</db:modifier>
<db:synopsisinfo role="access">public</db:synopsisinfo>
<db:synopsisinfo role="status">active</db:synopsisinfo>
<db:synopsisinfo role="threadsafeness">unspecified</db:synopsisinfo>
</db:fieldsynopsis>
<db:para>Enables the <db:link xlink:href="qml-qtwebview-webview.xml">WebView</db:link> to load file URLs.</db:para>
</db:section>
