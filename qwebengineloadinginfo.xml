<db:member><db:link xlink:href="qwebenginepage.xml#loadFinished">QWebEnginePage::loadFinished</db:link></db:member>
<db:member><db:link xlink:href="qml-qtwebengine-webengineview.xml#loadingChanged-signal">WebEngineView::loadingChanged</db:link></db:member>
</db:simplelist>
</db:para>
</db:section>
</db:section>
