<db:thead>
<db:tr>
<db:th>Constant</db:th>
<db:th>Value</db:th>
<db:th>Description</db:th>
</db:tr>
</db:thead>
<db:tr>
<db:td>
<db:para><db:code><db:emphasis role="bold"><db:link xlink:href="qt3drender.xml">Qt3DRender</db:link></db:emphasis>::<db:emphasis role="bold"><db:link xlink:href="qt3drender-qfrontface.xml">QFrontFace</db:link></db:emphasis>::ClockWise</db:code></db:para>
</db:td>
<db:td><db:code>0x0900</db:code></db:td>
<db:td>
<db:para>Clockwise polygons are front facing.</db:para>
</db:td>
</db:tr>
<db:tr>
<db:td>
<db:para><db:code><db:emphasis role="bold"><db:link xlink:href="qt3drender.xml">Qt3DRender</db:link></db:emphasis>::<db:emphasis role="bold"><db:link xlink:href="qt3drender-qfrontface.xml">QFrontFace</db:link></db:emphasis>::CounterClockWise</db:code></db:para>
</db:td>
<db:td><db:code>0x0901</db:code></db:td>
<db:td>
<db:para>Counter clockwise polygons are front facing.</db:para>
</db:td>
