<db:enumsynopsis>
<db:enumname>WindingDirection</db:enumname>
<db:enumitem>
<db:enumidentifier>ClockWise</db:enumidentifier>
<db:enumvalue>0x0900</db:enumvalue>
</db:enumitem>
<db:enumitem>
<db:enumidentifier>CounterClockWise</db:enumidentifier>
<db:enumvalue>0x0901</db:enumvalue>
</db:enumitem>
<db:synopsisinfo role="access">public</db:synopsisinfo>
<db:synopsisinfo role="status">active</db:synopsisinfo>
