<db:anchor xml:id="details"/>
<db:para>The Qt Canvas 3D functionality can be accessed via these QML types.</db:para>
<db:variablelist role="members">
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3d.xml" xlink:role="">Canvas3D</db:link></db:term>
<db:listitem>
<db:para>Canvas that provides a 3D rendering context.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dabstractobject.xml" xlink:role="">Canvas3DAbstractObject</db:link></db:term>
<db:listitem>
<db:para>Base type for Canvas3D types representing OpenGL resources.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dactiveinfo.xml" xlink:role="">Canvas3DActiveInfo</db:link></db:term>
<db:listitem>
<db:para>Active attribute or uniform information.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dbuffer.xml" xlink:role="">Canvas3DBuffer</db:link></db:term>
<db:listitem>
<db:para>Contains an OpenGL buffer.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dcontextattributes.xml" xlink:role="">Canvas3DContextAttributes</db:link></db:term>
<db:listitem>
<db:para>Attribute set for Context3D</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dframebuffer.xml" xlink:role="">Canvas3DFrameBuffer</db:link></db:term>
<db:listitem>
<db:para>Contains an OpenGL framebuffer.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dprogram.xml" xlink:role="">Canvas3DProgram</db:link></db:term>
<db:listitem>
<db:para>Contains a shader program.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3drenderbuffer.xml" xlink:role="">Canvas3DRenderBuffer</db:link></db:term>
<db:listitem>
<db:para>Contains an OpenGL renderbuffer.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dshader.xml" xlink:role="">Canvas3DShader</db:link></db:term>
<db:listitem>
<db:para>Contains a shader.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dshaderprecisionformat.xml" xlink:role="">Canvas3DShaderPrecisionFormat</db:link></db:term>
<db:listitem>
<db:para>Contains the shader precision format attributes.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dtexture.xml" xlink:role="">Canvas3DTexture</db:link></db:term>
<db:listitem>
<db:para>Contains an OpenGL texture.</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term><db:link xlink:href="qml-qtcanvas3d-canvas3dtextureprovider.xml" xlink:role="">Canvas3DTextureProvider</db:link></db:term>
