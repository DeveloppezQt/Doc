<db:varlistentry>
<db:term>Import Statement</db:term>
<db:listitem>
<db:para>import QtQml.StateMachine 1.0</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term>Since</db:term>
<db:listitem>
<db:para>Qt 5.4</db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
<db:term>Inherited By:</db:term>
<db:listitem>
<db:para><db:link xlink:href="qml-qtqml-statemachine-signaltransition.xml" xlink:role="">SignalTransition</db:link> and <db:link xlink:href="qml-qtqml-statemachine-timeouttransition.xml" xlink:role="">TimeoutTransition</db:link></db:para>
</db:listitem>
</db:varlistentry>
<db:varlistentry>
