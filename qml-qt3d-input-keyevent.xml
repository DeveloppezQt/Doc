<db:simplelist type="vert" role="see-also">
<db:member><db:link xlink:href="qkeysequence.xml#StandardKey-enum">QKeySequence::StandardKey</db:link></db:member>
</db:simplelist>
</db:para>
</db:section>
</db:section>
</db:section>
