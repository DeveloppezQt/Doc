<db:section xml:id="signalStrength-prop">
<db:title>[read-only] signalStrength : real</db:title>
<db:fieldsynopsis>
<db:type>real</db:type>
<db:varname>signalStrength</db:varname>
<db:modifier>[read-only]</db:modifier>

