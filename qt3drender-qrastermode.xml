<db:tr>
<db:td>
<db:para><db:code><db:emphasis role="bold"><db:link xlink:href="qt3drender.xml">Qt3DRender</db:link></db:emphasis>::<db:emphasis role="bold"><db:link xlink:href="qt3drender-qrastermode.xml">QRasterMode</db:link></db:emphasis>::Points</db:code></db:para>
</db:td>
<db:td><db:code>0x1B00</db:code></db:td>
<db:td>
