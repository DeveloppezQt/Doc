<db:section xml:id="transferProgress">
<db:title>void QBluetoothTransferReply::transferProgress(qint64 <db:emphasis>bytesTransferred</db:emphasis>, qint64 <db:emphasis>bytesTotal</db:emphasis>)</db:title>
<db:methodsynopsis>
<db:void/>
<db:methodname>transferProgress</db:methodname>
<db:methodparam>
<db:type>qint64</db:type>
<db:parameter>bytesTransferred</db:parameter>
</db:methodparam>
<db:methodparam>
<db:type>qint64</db:type>
<db:parameter>bytesTotal</db:parameter>
</db:methodparam>
