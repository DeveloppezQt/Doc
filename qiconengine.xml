<db:type>const QString &amp;</db:type>
<db:parameter>fileName</db:parameter>
</db:methodparam>
<db:methodparam>
<db:type>const QSize &amp;</db:type>
<db:parameter>size</db:parameter>
</db:methodparam>
<db:methodparam>
<db:type>QIcon::Mode</db:type>
<db:parameter>mode</db:parameter>
</db:methodparam>
<db:methodparam>
